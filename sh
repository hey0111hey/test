<script>/* plugin = comment_num2, hash=, key=cc2d2f0364edaabf8cc38e9f4080afcc */ atwiki_j$(function($){
    var plugin_name = 'comment_num2';
    var commentForm = $('form.plugin_'+ plugin_name +'_form');
    //投稿ボタンが押された時にtokenを取得、追加する
    commentForm.on('submit', function(event) {
        var num = commentForm.index(event.target);
        $.ajax({
            url: '/comment_token',
            dataType: 'jsonp',
            type: 'POST',
            data: {
                plugin_name: plugin_name
            },
            success: function(data) {
                commentForm.prepend($('<input />', {'type': 'hidden', 'name': 'token_time_'+ plugin_name, 'value': data['token_time_' + plugin_name]}));
                commentForm.prepend($('<input />', {'type': 'hidden', 'name': 'token_'+ plugin_name, 'value': data['token_' + plugin_name]}));
                commentForm.prepend($('<input />', {'type': 'hidden', 'name': 'token_key_'+ plugin_name, 'value': data['token_key_' + plugin_name]}));
            },
            error: function(){
            },
            complete: function() {
                commentForm.off('submit');
                commentForm[num].submit();
            }
        });
        return false;
    });
});</script>
