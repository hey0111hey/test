        tMenu = document.all[tName].style;
    }
    
    
    value = getCookie_treemenu2_8_6019(key);
    if ( value=='open' ) {
        tMenu.display = 'block';
    }
    if ( value=='close' ) {
	    tMenu.display = "none";
    }
    
    
}


    set_treemenu_treeMenu2_8_6019_f0d7f757b174ddd2bf572956cb89025f('treeMenu2_6019_8_f0d7f757b174ddd2bf572956cb89025f');
    

