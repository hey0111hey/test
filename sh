	if(document.all || document.getElementById){
		if(document.all){
			tMenu = document.all[tName].style;
		}else if(document.getElementById){
			tMenu = document.getElementById(tName).style;
		}else{
			tMenu = document.all[tName].style;
		}

        key = 'atwiki_treemenu2_openclose__17_2743';
    		if (tMenu.display == 'none') {
		    setCookie_treemenu2_17_2743(key, 'open');
		    tMenu.display = "block"; 
		} else {
		    setCookie_treemenu2_17_2743(key, 'close')
		    tMenu.display = "none";
		}
	}
}





function set_treemenu_treeMenu2_17_2743_9af3234bdf9dff9ca7ba37a666804758(tName) {
    key = 'atwiki_treemenu2_openclose__17_2743';
    if(document.all){
        tMenu = document.all[tName].style;
    }else if(document.getElementById){
        tMenu = document.getElementById(tName).style;
    }else{
        tMenu = document.all[tName].style;
    }
    
    
    value = getCookie_treemenu2_17_2743(key);
    if ( value=='open' ) {
        tMenu.display = 'block';
    }
    if ( value=='close' ) {
	    tMenu.display = "none";
    }
    
    
}



// --></script>
</li>
			
									
						
