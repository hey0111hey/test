    		if (tMenu.display == 'none') {
		    setCookie_treemenu2_1_6019(key, 'open');
		    tMenu.display = "block"; 
		} else {
		    setCookie_treemenu2_1_6019(key, 'close')
		    tMenu.display = "none";
		}
	}
}





