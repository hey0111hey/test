function getCookie_treemenu2_0_12049(key,  tmp1, tmp2, xx1, xx2, xx3) {
    tmp1 = " " + document.cookie + ";";
    xx1 = xx2 = 0;
    len = tmp1.length;
    while (xx1 < len) {
        xx2 = tmp1.indexOf(";", xx1);
        tmp2 = tmp1.substring(xx1 + 1, xx2);
        xx3 = tmp2.indexOf("=");
        if (tmp2.substring(0, xx3) == key) {
            return(unescape(tmp2.substring(xx3 + 1, xx2 - xx1 - 1)));
        }
        xx1 = xx2 + 1;
    }
    return("");
}

function setCookie_treemenu2_0_12049(key, val, tmp) {
    tmp = key + "=" + escape(val) + "; ";
    // tmp += "path=" + location.pathname + "; ";
    tmp += "path=/pazdra/; ";
    tmp += "expires=Tue, 31-Dec-2030 23:59:59; ";
    document.cookie = tmp;
}

function clearCookie_treemenu2_0_12049(key) {
    document.cookie = key + "=" + "xx; expires=Tue, 1-Jan-1980 00:00:00;";
}

function exMenu_treemenu2_0_12049_8257e6deee3e364500321d1c1a1721d0(tName) {
	if(document.all || document.getElementById){
		if(document.all){
			tMenu = document.all[tName].style;
		}else if(document.getElementById){
			tMenu = document.getElementById(tName).style;
		}else{
			tMenu = document.all[tName].style;
		}

        key = 'atwiki_treemenu2_openclose__0_12049';
    		if (tMenu.display == 'none') {
		    setCookie_treemenu2_0_12049(key, 'open');
		    tMenu.display = "block"; 
		} else {
		    setCookie_treemenu2_0_12049(key, 'close')
		    tMenu.display = "none";
		}
	}
}





function set_treemenu_treeMenu2_0_12049_8257e6deee3e364500321d1c1a1721d0(tName) {
    key = 'atwiki_treemenu2_openclose__0_12049';
    if(document.all){
        tMenu = document.all[tName].style;
    }else if(document.getElementById){
        tMenu = document.getElementById(tName).style;
    }else{
        tMenu = document.all[tName].style;
    }
    
    
    value = getCookie_treemenu2_0_12049(key);
    if ( value=='open' ) {
        tMenu.display = 'block';
    }
    if ( value=='close' ) {
	    tMenu.display = "none";
    }
    
    
}


    set_treemenu_treeMenu2_0_12049_8257e6deee3e364500321d1c1a1721d0('treeMenu2_12049_0_8257e6deee3e364500321d1c1a1721d0');
    

