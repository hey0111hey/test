function exMenu_treemenu2_6_2743_65d72ef0a82976336cc0298ba5469edd(tName) {
	if(document.all || document.getElementById){
		if(document.all){
			tMenu = document.all[tName].style;
		}else if(document.getElementById){
			tMenu = document.getElementById(tName).style;
		}else{
			tMenu = document.all[tName].style;
		}

        key = 'atwiki_treemenu2_openclose__6_2743';
    		if (tMenu.display == 'none') {
		    setCookie_treemenu2_6_2743(key, 'open');
		    tMenu.display = "block"; 
		} else {
		    setCookie_treemenu2_6_2743(key, 'close')
		    tMenu.display = "none";
		}
	}
}





