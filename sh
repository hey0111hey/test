function onw(tUrl){
    //Open New Window
    window.open(tUrl, '_blank','width=700,height=700,resizable=1,scrollbars=1,location=1');
    return false;
}


function formClear(targetElement){
    if(targetElement.value == targetElement.defaultValue){
        targetElement.value = "";
    }
}

