function exMenu_treemenu2_37_12049_6e35d468ca3f16e08d38dbcebf46b39a(tName) {
	if(document.all || document.getElementById){
		if(document.all){
			tMenu = document.all[tName].style;
		}else if(document.getElementById){
			tMenu = document.getElementById(tName).style;
		}else{
			tMenu = document.all[tName].style;
		}

        key = 'atwiki_treemenu2_openclose__37_12049';
    		if (tMenu.display == 'none') {
		    setCookie_treemenu2_37_12049(key, 'open');
		    tMenu.display = "block"; 
		} else {
		    setCookie_treemenu2_37_12049(key, 'close')
		    tMenu.display = "none";
		}
	}
