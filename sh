function exMenu_treemenu2_5_12049_6328db88fdaff41aeb389164daef5acc(tName) {
	if(document.all || document.getElementById){
		if(document.all){
			tMenu = document.all[tName].style;
		}else if(document.getElementById){
			tMenu = document.getElementById(tName).style;
		}else{
			tMenu = document.all[tName].style;
		}

        key = 'atwiki_treemenu2_openclose__5_12049';
    		if (tMenu.display == 'none') {
		    setCookie_treemenu2_5_12049(key, 'open');
		    tMenu.display = "block"; 
		} else {
		    setCookie_treemenu2_5_12049(key, 'close')
		    tMenu.display = "none";
		}
	}
}





function set_treemenu_treeMenu2_5_12049_6328db88fdaff41aeb389164daef5acc(tName) {
    key = 'atwiki_treemenu2_openclose__5_12049';
    if(document.all){
        tMenu = document.all[tName].style;
    }else if(document.getElementById){
        tMenu = document.getElementById(tName).style;
    }else{
        tMenu = document.all[tName].style;
    }
    
    
    value = getCookie_treemenu2_5_12049(key);
    if ( value=='open' ) {
        tMenu.display = 'block';
    }
    if ( value=='close' ) {
	    tMenu.display = "none";
    }
    
    
}


    set_treemenu_treeMenu2_5_12049_6328db88fdaff41aeb389164daef5acc('treeMenu2_12049_5_6328db88fdaff41aeb389164daef5acc');
    

